import React from 'react';
import styled from 'styled-components';
import S from '../../Atoms'

const Root = styled(S.div)``;
/**
 * Returns a <{{ pascalCase name }}> molecule element.
 * --- 
 * PLEASE 
 * FILL 
 * IN 
 * DETAILS
 * ---
 * @param {*} props Props to be passed to the <{{ pascalCase name }}> element
 */
const {{pascalCase name}} = props => {
  return (
    <Root {...props}>
      {{pascalCase name}}
    </Root>
  );
};

export default {{pascalCase name}};
export {
  {{pascalCase name}}
}
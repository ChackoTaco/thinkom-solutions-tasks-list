import React from 'react';
import styled from 'styled-components';

const Root = styled.{{lowerCase name}}``;
/**
 * Returns a <{{ lowerCase name }}> element.
 * The HTML <{{ lowerCase name }}> element 
 * --- 
 * PLEASE 
 * FILL 
 * IN 
 * DETAILS
 * ---
 * @param {*} props Props to be passed to the <{{ lowerCase name }}> element
 */
const {{pascalCase name}} = props => {
  return (
    <Root {...props}>
      {props.children}
    </Root>
  );
};

export default {{pascalCase name}};
export {
  {{pascalCase name}}
}